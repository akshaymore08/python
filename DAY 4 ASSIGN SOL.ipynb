    "for num in range(1042000,702648265):\n",
    "   order = len(str(num))\n",
    "   sum = 0\n",
    "   temp = num\n",
    "   while temp > 0:\n",
    "       temp = temp % 10\n",
    "       sum += temp ** order\n",
    "       temp //= 10\n",
    "       if num == sum:\n",
    "         print(\"The first arstrong number is:\",num)\n",
    "         break"
   ]
